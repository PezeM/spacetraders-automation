{
  "name": "spacetraders-automation-server",
  "version": "1.0.0",
  "description": "Typescript NodeJS automation tool for spacetraders.io API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && xcopy /e /k /i /y \"../../config\" \"dist/config\"",
    "watch": "tsc --watch",
    "start:prod": "yarn build && cd dist && cross-env NODE_ENV=production node index.js",
    "start:dev": "yarn build && cd dist && cross-env NODE_ENV=development node index.js"
  },
  "author": "PezeM",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "config": "^3.3.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "spacetraders-api-sdk": "^0.3.0",
    "typescript": "^4.2.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.35",
    "cross-env": "^7.0.3"
  }
}
